import "AllBidirectionalRelations.egl";
import "PropertiesCreation.egl";
operation Element propertiesGeneration(isInterface: Boolean, bidirectionalRelations: Set, pack: String, path: String): List{
	var implementations: List;
	var bidirectionalImplementations: List;
	var isBidirectional=false; 
	if (not bidirectionalRelations.isEmpty()){
		implementations=self.getAllBidirectionalRelations(isInterface, bidirectionalRelations, pack, path); 
		if (implementations.size>0){
			bidirectionalImplementations=implementations;	
			isBidirectional=true;	
		}//if-implementations-size
	}//if-not-empty
    return self.classPropertiesCreation(path, pack, isBidirectional, bidirectionalImplementations);
}
%]					