// Description: This operation returns the code to create
//				non empty folders in the Visual Studio project 
////////////////////////////////////////////////////////////
 
operation nonEmptyPackages(packages:Set):String { 
	var code;
  	for (path in packages){
  		code=code+"<Compile Include=\""+path+"\" />\n\t";
  	}
  	return code;
}
%]