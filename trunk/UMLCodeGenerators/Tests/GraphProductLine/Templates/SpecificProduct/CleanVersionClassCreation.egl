import "../ClassElements/ModelCreation.egl"; 
%]
using System;
using System.Collections.Generic;
[%
var p=currentPackage.name; 
var name;
if (isInterface){
	name="interface";
}else{
	name="class";
}
%]
// This is the content of the [%=name%] named "[%=currentElement%]", which is 
// in the package: "[%=p%]", from the "[%=currentModel.name%]" model. 
[%=currentModel.modelDeclaration()%]
	public [%=name%] [%=currentElement%]{ 
		// Constructor and methods from the from the current class		
		[%for (method in currentMethods){%]
		[%=method%]
		[%}%]	
	 
	}
} 
  