    <NodeDisplayFormat value="GroupNode">
        <Shape value="dot"/>
        <FillColor value="lightgreen"/><!-- O.R. It would be fine to have it changeable according to the group cardinality. Is it worth doing? -->
        <!-- O.R. Line makes no sense for Group -->
        <!-- O.R. Element <TypeText/> is omited - see the comment in DisplayMetamodel.xsd -->
        <!-- O.R. Let me note that NameText element defines the way how the name of the group is displayed in the "property editing area" -->
        <!-- <NameText/> missing -->
        <!--O.R.(see the comment in DisplayMetamodel.xsd)<PropertySetText HorizontalPosition="left-corner" VerticalPosition="top-corner" value="black" font="default" FontSize="small" FontStyle="normal" Underline="no"/>-->
        <CardinalityText>
            <VerticalPosition value="bottom-outside"/>
            <HorizontalPosition value="right-outside"/>
            <Color value="green"/>
            <FontStyle value="bold"/>
            <Underline value="no"/>
            <FontName value="default"/>
            <FontSize value="normal"/>
            <Line>
                <Color value="black"/>
                <Width value="thick"/>
                <Style value="solid"/>
            </Line>
            <FillColor value="transparent"/>
        </CardinalityText>
        <AttachedToParent/>
    </NodeDisplayFormat>
    
