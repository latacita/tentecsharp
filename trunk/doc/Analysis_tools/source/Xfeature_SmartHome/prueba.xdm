    <NodeDisplayFormat value="FeatureModel">
        <Shape value="round"/>
        <FillColor value="#00C000"/>
        <Line>
            <Color value="black"/>
            <Width value="thick"/>
            <Style value="solid"/>
        </Line>
        <NameText>
            <!-- <DisplayString value="#fm:BasicProperySet/fm:FeatureNameProperty/@fm:value"/> proposal Jan 9 -->
            <VerticalPosition value="center"/>
            <HorizontalPosition value="center"/>
            <Color value="black"/>
            <FontStyle value="bold"/>
            <Underline value="no"/>
            <FontName value="default"/>
            <FontSize value="large"/>
        </NameText>
        <CardinalityText>
            <VerticalPosition value="top-outside"/>
            <HorizontalPosition value="right-outside"/>
            <Color value="red"/>
            <FontStyle value="bold"/>
            <Underline value="no"/>
            <FontName value="default"/>
            <FontSize value="normal"/>
            <Line>
                <Color value="black"/>
                <Width value="normal"/>
                <Style value="solid"/>
            </Line>
            <FillColor value="yellow"/>
        </CardinalityText>        
        <RootNode/>
    </NodeDisplayFormat>
    
    <NodeDisplayFormat value="GroupedFeatureNode">
        <Shape value="round"/>
        <FillColor value="lightyellow"/>
        <Line>
            <Color value="black"/>
            <Width value="normal"/>
            <Style value="dash"/>
        </Line>
        <NameText>
            <!-- <DisplayString value="#fm:BasicProperySet/fm:FeatureNameProperty/@fm:value"/> proposal Jan 9 -->
            <VerticalPosition value="center"/>
            <HorizontalPosition value="center"/>
            <Color value="black"/>
            <FontStyle value="bold"/>
            <Underline value="no"/>
            <FontName value="default"/>
            <FontSize value="large"/>
        </NameText>
        <!-- SOLVED O.R. Element <TypeText/> is omited - see the comment in DisplayMetamodel.xsd -->
        <!-- SOLVED [TBW] O.R. Let me note that NameText element defines the way how the name of the feature is displayed 
        but does not specify whether it ref to display in "property editing area" or in "node editing area" -->
        <!--<PropertySetText DisplayString=""/>-->
        <!--SOLVED O.R. (you can always ignote inforamtion) (see the comment in DisplayMetamodel.xsd)<PropertySetText HorizontalPosition="left-corner" VerticalPosition="top-corner" value="black" font="default" FontSize="small" FontStyle="normal" Underline="no"/>-->
        <CardinalityText>
            <VerticalPosition value="top-outside"/>
            <HorizontalPosition value="right-outside"/>
            <Color value="red"/>
            <FontStyle value="bold"/>
            <Underline value="no"/>
            <FontName value="default"/>
            <FontSize value="normal"/>
            <Line>
                <Color value="black"/>
                <Width value="normal"/>
                <Style value="solid"/>
            </Line>
            <FillColor value="yellow"/>
        </CardinalityText>
        <ParentConnector>
            <Color value="black"/>
            <Width value="normal"/>
            <Style value="solid"/>
        </ParentConnector>
    </NodeDisplayFormat>
    
