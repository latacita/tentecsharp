// Description: This operation returns the parameters for 
//				the current method. 
////////////////////////////////////////////////////////////
import "Operations.eol";
@template
operation Feature parameters() { 
	// Method's parameters
	var params: String;
	var parameters: OrderedSet;	
	// Reject blank and return parameters
	parameters := self.ownedParameter.reject(p|p.name.isEmpty() or p.direction.toString().equals("return"));		
	// Add to the var params each parameter from the given method
	for (param in parameters){	
		if(param.upperBound==-1 or param.upperBound>1){
			// If the parameter is a collection
			var collection=param.typeCollection();
			params=params+" "+collection+" "+param.name+",";
		}else{				
			params=params+" "+param.type.name+" "+param.name+",";	
		}							
	}
	// Deleting the last "," of the string, if needed
    if (not params.isEmpty()){	
    	params = params.substring(0, params.length() - 1);
    }%]
	[%=params%] 
[%} %]		
 
   
